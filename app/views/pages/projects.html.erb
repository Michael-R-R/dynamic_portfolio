<% provide(:title, "Projects") %>
<% content_for(:head) do %>
  <%= stylesheet_link_tag("shared/post_preview", "data-turbo-track": "reload") %>
  <%= javascript_import_module_tag("public/Projects")%>
<% end %>
<%= hidden_field_tag("show_count", @show_count,  { :id => "show-count" }) %>
<%= hidden_field_tag("max_count", @max_count,  { :id => "max-count" }) %>

<div class="h-container">
    <div class="v-container">
        <div class="v-container-item">
            <nav id="v-nav-projects-preview" class="v-nav">
                <%= render(:partial => "shared/post_preview", :locals => { :posts => @p_posts }) %>
            </nav>
        </div>
    </div>
</div>
