<% provide(:title, "Create Project Post") %>

<div class="v-container">
  <div class="v-container-item">
    <%= form_with(url: create_project_path, model: @post) do |f| %>
        <%= f.label(:title) %>
        <%= f.text_field(:title) %>

        <%= f.label(:thumbnail) %>
        <%= f.text_field(:thumbnail) %>

        <%= f.label(:body) %>
        <%= f.text_area(:body) %>

        <%= f.submit("Create") %>
    <% end %>
  </div>
</div>